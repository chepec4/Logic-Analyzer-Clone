TARGETNAME=C4Analyzer
TARGETPATH=obj
TARGETTYPE=DYNLINK
UMTYPE=windows
DLLDEF=main.def

# Legacy MSVC Flags (Reference only)
MSC_WARNING_LEVEL=/W4 /WX /WL
386_OPTIMIZATION=/arch:SSE
AMD64_OPTIMIZATION=/favor:INTEL64
NOT_LEAN_AND_MEAN=1
USE_MSVCRT=1

# Sources used in the build
SOURCES=main.cpp vstsdk-wrapper.cpp main.rc

# Include paths logic
INC=..\libraries
INCLUDES=rc;            \
         $(INC);        \
         $(INC)\win;    \
         $(VSTSDKROOT); \
         $(VSTSDKROOT)\public.sdk\source\vst2.x
